<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ru.arkuzmin.costtracker.controller.CostsController">
  <!-- TODO Add Nodes -->
  <children>
    <BorderPane prefHeight="400.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <center>
        <TableView fx:id="costTable" prefHeight="200.0" prefWidth="200.0">
          <columns>
            <TableColumn prefWidth="75.0" text="Название" fx:id="costColName" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="91.0" text="Категория" fx:id="costColCategory" />
            <TableColumn prefWidth="75.0" text="Агент" fx:id="costColAgent" />
            <TableColumn prefWidth="75.0" text="Сумма" fx:id="costColAmount" />
            <TableColumn prefWidth="75.0" text="Дата" fx:id="costColDate" />
          </columns>
        </TableView>
      </center>
      <top>
        <Pane prefHeight="50.0" prefWidth="800.0">
          <children>
            <Button layoutX="684.0" layoutY="16.0" mnemonicParsing="false" onAction="#editCost" text="Редактировать" />
          </children>
        </Pane>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
